{
  "source": {
    "include": ["./src"]
  },
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./doc",
    "verbose": true,
    "recurse": true,
    "template": "node_modules/minami"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true,
    "default": {
      "outputSourceFiles": true
    },
    "navType": "vertical",
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
  }
}
